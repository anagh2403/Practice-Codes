        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Sample dataset for the heatmap\n",
        "data = [[0.5, 0.2, 0.9],\n",
        "        [0.1, 0.6, 0.4],\n",
        "        [0.3, 0.8, 0.7]]\n",
        "\n",
        "# Create a heatmap using seaborn\n",
        "sns.heatmap(data)\n",
        "\n",
        "# Display the heatmap\n",
        "plt.show()"
      ]
    }
  ]
